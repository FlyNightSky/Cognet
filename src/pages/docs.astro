---
import Layout from "../layouts/Layout.astro";
import docCard from "./components/docsCard.astro";
---

<Layout title="Docs">
    <nav class="fixed top-0 left-0 right-0 z-50 backdrop-blur-[.2rem] h-[6.5rem] text-white overflow-hidden	">
        <a href='https://github.com/FlyNightSky/cognet'><img src="github.svg" alt="githubLogo" class="float-right mt-7 mx-4 w-12" id="github"></a>
        <a href="./proxys" class="text-white float-right mt-10 pl-5 pr-2 hover:text-primary">Proxys</a>
        <a href="./docs" class="text-white float-right mt-10 hover:text-primary">Docs</a>
        <a href="./"><img src="/logo.png" alt="logo"class="w-24 pb-5 pt-5 px-3" id="logo"></a>
    </nav>

   <div class="flex justify-start">
  <div class="mt-[10rem] pr-[3rem] h-10 absolute p-2 ml-5 bg-primary opacity-60 rounded-t-lg rounded-l-lg rounded-r-lg text-white hover:opacity-100 md:p-3 xl:p-3">Welcome!</div>

  <a onclick="#" class="inline-flex items-center px-3 py-2 text-center text-white mt-[13rem] p-2 pr-[1rem] ml-5 bg-primary opacity-60 rounded-t-lg rounded-l-lg rounded-r-lg z-0 hover:opacity-100 md:p-3 xl:p-3">
            Get Started

            <svg aria-hidden="true" class="w-4 h-4 ml-2 -mr-1" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
        </a>

         <docCard/>

   
   


</Layout>

<style>
    html,body{
        background-color: black;
        font-family:Poppins;
        font-weight: bold;
    
    }

    .btn1{
		@apply p-2 mt-5 bg-docsRed rounded-t-lg rounded-l-lg rounded-r-lg  shadow-secondary-200 hover:opacity-75 shadow-lg lg:p-3 md:p-3 xl:p-3;
	}
</style>